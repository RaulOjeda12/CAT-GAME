#include<stdio.h>
#include<conio.h>
int f,c;
int matriz[3][3]={0};
int main(void)
{
    //Hacer el tablero//
printf("\t\tGATO\n");
for(f=0;f<3;f++)
    {
        printf("\n");
for(c=0;c<3;c++)
    {
    printf("%d\t",matriz[f][c]);
    if(c<2)
    {
        printf("|");
    }
    } 
}  
//FUNCIONES Y CONDICIONES//
return 0;
}  
